def draw(level1):
    i = 0
    while i<len(level1):
        j = 0
        while j<len(level1[i]):
            if level1[i][j]==1:
                sys.stdout.write('x')
            elif level1[i][j] == 0:
                sys.stdout.write(' ')
            else:
                sys.stdout.write('z')
            j=j+1
        i=i+1
        print()
        

draw(level1)
level1[19][0] ='z'
print(level1[19][0])
draw(level1)
